<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.fightclub" name="Fight Club" version="0.0.1" provider-name="SteveRob">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.urlresolver" version="0.0.1"/>
		<import addon="script.module.universal" version="0.0.1" />
	</requires>
	<extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Fight Club</summary>
		<description lang="en">Fight Club (streams from wweo.net) - The biggest WWE and Wrestling TV Show and videos Database. Watch WWE, TNA, UFC, FCW, ROH for free.</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="repository.steverob" name="SteveRob Repo" version="1.0" provider-name="SteveRob">
	<extension point="xbmc.addon.repository" name="SteveRob Repo">
		<info compressed="false">http://github.com/dev-steverob/kodi/raw/master/addons.xml</info>
		<checksum>http://github.com/dev-steverob/kodi/raw/master/addons.xml.md5</checksum>
		<datadir zip="true">http://github.com/dev-steverob/kodi/raw/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install MY Addons</summary>
		<description>Download and install addons from SteveRob.</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
